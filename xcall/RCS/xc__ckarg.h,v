typedef
struct					/* XCALL Argument Descriptor	*/
  {					/* ----------------------------	*/
    int       minsize;			/* Minimum size of arg data	*/
    int       maxsize;			/* Maximum size of arg data	*/
    char     *dtype;			/* Data type ('A', 'D', etc)	*/
    int       flags;			/* ARG_ISxxx or 0=any		*/
    char     *ercode;			/* Error message code if ! match*/
  }       XC_ADscr;

