1.74
log
@1996-Apr-07 Checkpoint
@
text
@;;;
; $Source: /home/ldl/dbl/cdbl/RCS/hello.dbl,v $
; $Revision: 1.73 $ $Date: 1996/03/08 12:53:56 $
;
; Our first executing program.

RECORD PROG_VARS
	MSG	, A18, 'Hello there, world'
	I	, D2, 0

PROC
	DISPLAY(1, "CALL 'SUB'", 10)
	CALL SUB
	DISPLAY(1, "BACK FROM 'SUB'", 10)
	WHILE (I .LT. 10)
	  BEGIN
	    DISPLAY (1, MSG, 10)
	    INCR I
	  END
	DISPLAY(1, "CALL 'SUB'", 10)
	CALL SUB
	DISPLAY(1, "BACK FROM 'SUB'", 10)
	STOP
SUB,
	DISPLAY(1, "THIS IS FROM SUBROUTINE 'SUB'", 10)
	RETURN

; End of $Source: /home/ldl/dbl/cdbl/RCS/hello.dbl,v $
@


