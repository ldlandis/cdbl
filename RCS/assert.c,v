@1996-Apr-07 Checkpoint
@
text
@/***
 * $Source: /home/ldl/dbl/cdbl/RCS/assert.c,v $
 * $Revision: 1.73 $ $Date: 1996/03/08 12:53:46 $
 */

/***
 *	This implements a localized "assert".
 */

#include <stdio.h>
#include "prototype.h"
#include "assert.h"


/***
 *	Local variables.
 */
static
int       failing = 0;			/* Failure in progress		*/

void
_ASSERT_H_(p, n1, n2, assertion, file, line)		/* Function	*/
  char     *p;
  int       n1, n2;
  char     *assertion;
  char     *file;
  int       line;
{
  c_error("%s:%d Assertion Failed: %s\n", file, line, assertion);
  printf(" %c = %d  ", p[0], n1);
  if (strlen(p) == 2)
    printf(" %c = %d  ", p[1], n2);
  printf("\n");
  if ( failing++ )			/* It's already failing		*/
    exit( 1 );
  { /* block */
    extern
    int       inhibit_c_dump_nodes,
	      divide_by_zero_on_assertion_failure;
    if (divide_by_zero_on_assertion_failure)
      { int x = 1 / 0; }
    if (inhibit_c_dump_nodes)
      printf("node dump inhibited\n");
    else
      node_print(stdout, 1, -1);
  } /* block */
} /* _assert_ */

/* End of $Source: /home/ldl/dbl/cdbl/RCS/assert.c,v $ */
@


