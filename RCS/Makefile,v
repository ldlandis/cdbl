1.74
log
@1996-Apr-07 Checkpoint
@
text
@###
# $Source: /home/ldl/dbl/cdbl/RCS/Makefile,v $
# $Revision: 1.73 $ $Date: 1996/03/08 12:53:45 $
#

.SUFFIXES: .dbl
.dbl.c:
	../bin/cdbl -g -t $< >`basename $@@ .c`.sym

HEADERS	= \
	assert.h \
	cdbl.h\
	cdbl_rt.h\
	expr.h\
	node.h \
	parse.h \
	prototype.h\
	util.h

SRC	=\
	_tokens.c\
	assert.c\
	cdbl.c\
	cdbl_rt.c\
	expr.c\
	generate.c\
	gram.g\
	gram_support.c\
	lex.l\
	node.c\
	resolve.c\
	util.c

LIBRARY	= libcdbl.a
LIB	=\
	$(LIBRARY)(cdbl_rt.o)

CFLAGS	= -pedantic -DYYDEBUG -I.. -g -static
#CFLAGS	= -pedantic -DYYDEBUG -I..
LFLAGS	= -I
YFLAGS	= -vdt

.SUFFIXES: .g
.g.c:
	yacc $(YFLAGS) $<
	@@echo "See notes in $< regarding shift/reduce conflicts"
	sed -e "/yylex/s/0;/0; yystate=*yyssp;/" < y.tab.c > gram.c
	@@echo "making gram.h"
	@@echo "/***"						>  gram.h
	@@echo " * DO NOT EDIT NOR ARCHIVE THIS FILE."		>> gram.h
	@@echo " * IT IS AUTOMATICALLY CREATED. See Makefile."	>> gram.h
	@@echo " */"						>> gram.h
	@@echo "#ifndef _GRAM_H_"				>> gram.h
	@@echo "#define _GRAM_H_"				>> gram.h
	@@echo "#include \"node.h\""				>> gram.h
	@@cat y.tab.h						>> gram.h
	@@echo "#endif/*_GRAM_H_*/"				>> gram.h
	@@echo "/* End of automatically generated file */"	>> gram.h

what:	qa

all:	node_qa qa_kp cdbl_di cdbl lib
	#@@echo ======================================================
	#(export YYDEBUG=9 ; cdbl -p -r -g clrbox.dbl)
	#@@echo ======================================================
	#(                   cdbl clrbox.dbl)
	#@@echo ======================================================
	@@echo "'all' done"

install:	all
	mv cdbl ../bin
	cp libcdbl.a ../lib

lib:	$(LIBRARY)
$(LIBRARY): $(LIB) builtin/* xcall/*
	(cd builtin; make lib)
	(cd xcall;   make lib)

qa:	install
	(cd qa; make test)

tags:	$(SRC)
	ctags -t $(SRC)

# The compiler
assert.o: assert.c			$(HEADERS)
cdbl.o: cdbl.c				$(HEADERS)
expr.o: expr.c				$(HEADERS)
generate.o: generate.c			$(HEADERS)
gram.c gram.h: gram.g			$(HEADERS)
gram.o:	gram.c gram_support.c		$(HEADERS)
lex.o:	lex.l _tokens.c			$(HEADERS)
node.o:	node.c _tokens.c		$(HEADERS)
resolve.o: resolve.c			$(HEADERS)
util.o:	util.c				$(HEADERS)

# The Run-time
cdbl_rt.o: cdbl_rt.c			$(HEADERS)
	$(COMPILE.c) -DCDBL_RT_DEBUG $< $(OUTPUT_OPTION)

_tokens.c:	Makefile gram.c gram.h mktokens.awk mktokens.map
	cat y.tab.h mktokens.map | awk -f mktokens.awk > _tokens.c
	@@echo "making _tokens.h"
	@@echo "/***"						>  _tokens.h
	@@echo " * DO NOT EDIT NOR ARCHIVE THIS FILE."		>> _tokens.h
	@@echo " * IT IS AUTOMATICALLY CREATED. See Makefile."	>> _tokens.h
	@@echo " */"						>> _tokens.h
	@@echo "extern"						>> _tokens.h
	@@echo "char *token_rkw[], *token_kw[], *tokenseq[];"	>> _tokens.h
	@@echo "/* End of automatically generated file */"	>> _tokens.h

CDBL_O	= assert.o expr.o generate.o gram.o lex.o\
	  node.o resolve.o util.o _tokens.o
cdbl:	always $(CDBL_O)
	make CFLAGS="-DIS_MAIN" cdbl.o
	$(CC) -o cdbl cdbl.o $(CDBL_O)

NODE_QA	= node_qa.o assert.o _tokens.o util.o
node_qa:	always $(NODE_QA)
	$(CC) -o node_qa  $(NODE_QA)
	node_qa > node_qa.out

node_qa.o: node.c $(HEADERS)
	make CFLAGS="-DMODULE_TEST -DIS_MAIN" node.o
	mv node.o node_qa.o

asg_num:	asg_num.c	Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

bi_qa:		bi_qa.c		Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

iio:		iio.c		Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

io:		io.c		Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

io_open:	io_open.c	Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

lucase:	lucase.c		Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

subs:	subs.c			Makefile libcdbl.a
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

xc_qa:	xc_qa.c			Makefile lib
	$(CC) $(CFLAGS) -o $@@ $< libcdbl.a -ldb

cdbl_di: cdbl_rt.c cdbl_rt.h	Makefile
	$(CC) $(CFLAGS) -DCDBL_RT_DEBUG -DUTL_DUMP_INDEX cdbl_rt.c\
	  -o cdbl_di -ldb

qa_kp:	cdbl_rt.c cdbl_rt.h	Makefile
	$(CC) $(CFLAGS) -DCDBL_RT_DEBUG -DQA_PARSE_KEYS cdbl_rt.c\
	  -o qa_kp

qa_kp_test: qa_kp
	qa_kp "((1,2))"

always:

clean:
	-(cd qa; make clean)
	-(cd xcall; make clean)
	rm -f {.,}*.{bak,swp,o,out,sym} _tokens.[ch] gram.[ch]
	rm -f CDBL.pst* mak.log tags y.tab.h node_qa* diffs
	rm -f cdbl $(LIBRARY)
	rm -f cdbl_di demo.log lex.c qa_kp y.output y.tab.c
	rm -f asg_num{,.c}
	rm -f bi_qa{,.c}
	rm -f hello{,.c}
	rm -f io{,.c,.ddf,.lst}
	rm -f iio{,.c,.ddf,.lst}
	rm -f lucase{,.c}
	rm -f subs{,.c}
	rm -f xc_qa{,.c}

diffs:	Makefile *.[chgl]
	doRCSprocess rcsdiff > diffs 2>&1


# End of $Source: /home/ldl/dbl/cdbl/RCS/Makefile,v $
@


