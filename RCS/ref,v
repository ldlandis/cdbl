1.74
log
@1996-Apr-07 Checkpoint
@
text
@afield=aexp
nfield=nexp
nfield=aexp
afield=nexp
afield=nexp,format
field=

ACCEPT (ch,field{,label}{,WAIT{:seconds}})
BEGIN
  stmts
END
CALL label
CLEAR field{,...}
CLOSE ch{,ch...}
DECR nvar
DELETE (ch)
DETACH
DISPLAY (ch,|aexp|{attrs}{,...})
            |nexp|
	attrs ::= <{POS:(r,c){,VIDEO:flags}{,ERASE:area}{,FORMAT:mask}>
	r     ::= row
	c     ::= column
	flags ::= 1=Blink 2=BOLD 4=REVERSE 8=UNDERSCORE
	area  ::= 0=ignored 1=screen 2=eos 3=line 4=eol
	mask  ::= C=any X=digit Z=zero-suppressed *=check-protected
	          money='$'=floating minus='-' decpt='.' digsp=','
DO
  stmt
UNTIL condition

EXIT {label}
EXITLOOP
        |{,RFA:rfaddr}                     |
FIND (ch|{,mval}{,KEYNUM:nexp}{,MATCH:nexp}|{,WAIT{:seconds}})
        KEYNUM:0=primary 1=1st alt, 2=2nd alt, etc
	MATCH:1=EQ  0=EQNXT 2=NEXT
FLUSH nexp
FOR nfield FROM initial THRY final {BY step} stmt
FORMS (ch,nexp)
        nexp 0=FF(12), 1-255=# CR-LF(13-10) -1=VT(11) -3=CR(13)
FRETURN expr
GOTO label
GOTO (label{,...}){,}nexp
IF expr stmt
IF expr THEN stmt ELSE stmt
INCR nfield
LOCASE afield
LPQUE (filespec{,LPNUM:exp}{,COPIES:nexp}{,FORM:exp}{,DELETE{:nexp}}{,ALIGN})
NEXTLOOP
NOP
OFF ERROR
OFFERROR
ONERROR label
ON ERROR label
OPEN (ch|{,mode{:submode}|,filespec{attrs})
        |MODE:mexp       |
	mode    ::= A=append I=input O=output U=update
	submode ::= S=sequential P=print R=relative I=indexed
	            B=block C=character *=wildchar
	attrs ::= {,ALLOC:nexp}
	          {,BKTSIZ:nexp}
                  {,BLKSIZ:nexp}
		  {,BUFNUM:nexp}
		  {,BUFSIZ:nexp}
		  {,RECSIZ:nexp}
		  {,CONTIG:nexp}
		  {,DEQ:nexp}
		  {,PROTECT:astr}
		  {,RECTYPE:nexp}
		  {,SHARE:nexp}
		  {,KEY:(keylist)}
		  {,FDL:fdlstr}
		  {,NUMREC:nexp}
PROC {(nlit)}
 stmts
{END}

indexed
READ (ch,record,|keyfld{,KEYNUM:nexp}{,MATCH:nexp}|{,WAIT{:seconds}})
                |RFA:rfaddr                       |
relative
READ (ch,record|,nexp      |{,WAIT{:seconds}})
               |,RFA:rfaddr|
sequential
READ (ch,record,RFA:rfaddr)

READS (ch,record{<attrs>}{,label}{,WAIT{:seconds}})
	attrs ::= POS:(r,c)
	        | ,VIDEO:vflags
		| ,ERASE:area
		| ,EDIT:maskk
		| ,DEFAULT:exp
		| ,FLAGS:flags
		| ,OFFSET:nexp
		| ,PROMPT:aexp
		| outattrs see display
        r     ::= row
	c     ::= column
	vflags::= 1=blink 2=bold 3=reverse 4=underscore
	area  ::= 1=clr 2=eos 3=line 4=eol
	mask  ::= C=any P=printable A=A-Za-z U=A-Z L=a-z X=0-9 
	          Y=A-Za-z0-9 [ 0]=use-dflt N=[.-+]
	flags ::= 1=ECHO 2=NOECHO 4=AUTO 8=NOAUTO 16=UPCASE 32=NOUPCASE
	          64=TERMECHO echo READS terminator 128=NOTERMECHO
		  256=TYPAHD 512=NOTYPAHD


RECV(message,label{,size})
REPEAT stmt
RETURN
SEND(message,program{,terminal})
SLEEP seconds
STOP {|status}           |}
     {|filespec}         |}
     {|@@cmdfil           |}
     {|@@cmdfil prmtr-list|}
     {|$ dcl-cmd-ln      |}
STORE(ch,record)
UNLOCK ch
UPCASE afield
USING expr SELECT
  ({mexp{,...}),stmt
  ...
ENDUSING
	mexp  ::= |value                   |
		  |value THRU value        |
		  |.LE., .LEU., .LES. value|
		  |.LT., .LTU., .LTS. value|
		  |.GE., .GEU., .GES. value|
		  |.GT., .GTU., .GTS. value|
		  |.EQS. value             |
		  |.EQU. value             |
USING sexp RANGE low THRU high SELECT
  ({mexp{,...}),stmt
  ...
ENDUSING

WHILE expr stmt

Indexed
WRITE(ch,record) 

Relative
WRITE(ch,record|,nexp      |)
               |,RFA:rfaddr|

Sequential
WRITE(ch,record, RFA:rfaddr)

WRITES(ch,record{<attr>})

XCALL name{(arg{,...})}
XRETURN

%ABS(exp)
%ADDR(field)
%ARGA(m)
%ARGN(m)
%B(value)
%BIN(exp{,flag}_
%BIT_IS_CLR(bit,value)
%BIT_IS_SET(bit,value)
%BKSTR(start,str_2_srch,str_2_srch_4)		DBL$BKSTR
%CHAR(exp)
%CHOPEN(ch)
%CPUTIME
%D(value)
%DATE
%DECML(string)
%DEFINED(field)
%DESCR(arg)
%ERLINE
%ERROR
%HEX(exp{,flag})
%INRANGE
%INSTR(start,str_2_srch,str_2_srch_4)
%INT(exp)
%INTEGER(exp)
%JBNO
%LINUX
%NUMARGS
%O(value)
%OCT(exp{,flag})
%OUTRANGE
%PACKED(exp)
%PASSED(argname)
%REF(arg)
%RFA
%RSIZE
%RTERM
%SIZE(field)
%STRING(exp)
%SUCCESS(value)
%SYSERR
%TIME
%TNMBR
%TTNAME
%TTSTS(ch)
%UNSIGNED(exp)
%VAL(|arg|)
     |exp|
%VARIANT
%VERSN
%VMS
%WAIT(parameters{,sec})
%WKDAY
%X(value)
            |+|
%XTRNL (arg{|-||arg |})
            |*||dlit|
	    |/|
%ZONED(exp)



    int       n_type;			/* Type of node			*/
" quote
" "
"\t"
#
##

%
& continuation
'
+
-
/
//
; comment

<id> = [a-z$][a-z0-9$_]* 
<lit> = "O'Hare" 'O''Hare'
<num> = {+|-}d{d}*.?{d}*


%ABS(exp)
%ADDR(field)
%ARGA(m)
%ARGN(m)
%B(value)
%BIN(exp[,flag])
%BIT_IS_CLR(bitnum,value)
%BIT_IS_SET(bitnum,value)
%BBBKSTR(start_pos,string,substring)
%CHAR(exp)
%CHOPEN(ch)
%CPUTIME
%D(value)
%DATE
%DECML(string)
%DEFINED(field)
%DESCR(argname)
%ERLINE
%ERROR
%HEX(exp[,flag])
%INRANGE
%INSTR(start_pos,string,substring)
%INT(exp)
%INTEGER(exp)
%JBNO
%NUMARGS
%O(value)
%OCT(exp[,flag])
%OUTRANGE
%PACKED(exp)
%PASSED(argname)
%REF
%RFA
%RSIZE
%RTERM
%SIZE(field_name)
%STRING(exp)
%SUCCESS(status)
%SYSERR
%TIME
%TNMBR
%TTNAME
%TTSTS(ch)
%UNSIGNED(exp)
%VAL(exp)
%VARIANT
%VERSN
%VMS
%WAIT(parameterss[,seconds])
%WKDAY
%X(value)
%XTRNL(exp)
%ZONED(exp)
ACCESS
